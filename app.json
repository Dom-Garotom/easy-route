{
  "expo": {
    "name": "rota-facil",
    "slug": "rota-facil",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon/icons-easy-route.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon/icon-easy-route.png",
        "backgroundColor": "#071E2C"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./src/assets/splash/spalsh-easy-route2.png",
      "resizeMode": "contain",
      "backgroundColor": "#071E2C"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
